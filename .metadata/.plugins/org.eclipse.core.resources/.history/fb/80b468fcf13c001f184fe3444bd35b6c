
public class SungJukMgmt1 {

	public static void main(String[] args) {
		Student [] array = new Student[2];
		java.util.Scanner sc = new java.util.Scanner(System.in);
		for(int i = 0; i < 3; i++) {
			array[i] = new Student();		// 학생객체 생성
			System.out.print("학번 : "); array[0].hakbun = sc.next();
			System.out.print("국어 : "); array[0].kor = sc.next();
			System.out.print("영어 : "); array[0].eng = sc.next();
			System.out.print("수학 : "); array[0].math = sc.next();
			array[i].hakbun = hakbun; 
		}

	}

}
